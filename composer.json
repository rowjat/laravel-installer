{
  "name": "rowjat/laravel-installer",
  "description": "Laravel web installer",
  "license": "MIT",
  "type": "library",

  "authors": [
    {
      "name": "Rowjat Ullah",
      "email": "rowjat@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12",
    "illuminate/console": "^12"
  },
  "autoload": {
    "psr-4": {
      "Rowjat\\Installer\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
    "extra":{
        "laravel":{
            "providers":[
                "Rowjat\\Installer\\InstallerServiceProvider"
            ]
        }
    },
  "minimum-stability": "dev"
}
